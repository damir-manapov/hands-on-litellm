# Required for OpenAI models (used by LiteLLM Docker Compose)
OPENAI_API_KEY=sk-your-actual-openai-api-key-here

# Required for Example 17: GigaChat integration
GIGACHAT_AUTH_KEY=your-gigachat-authorization-key-here

# Optional: LiteLLM Configuration
# LiteLLM master key (default: sk-1234)
LITELLM_MASTER_KEY=sk-1234

# LiteLLM salt key for encryption (default: sk-5678)
LITELLM_SALT_KEY=sk-5678

# LiteLLM proxy URL (default: http://localhost:4000)
LITELLM_BASE_URL=http://localhost:4000

# LiteLLM API key for authentication (same as LITELLM_MASTER_KEY)
LITELLM_API_KEY=sk-1234

# Optional: PostgreSQL Configuration (for Docker Compose)
# PostgreSQL database name (default: litellm)
POSTGRES_DB=litellm

# PostgreSQL user (default: litellm_user)
POSTGRES_USER=litellm_user

# PostgreSQL password (default: litellm_password)
POSTGRES_PASSWORD=litellm_password
