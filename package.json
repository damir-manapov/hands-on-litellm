{
  "name": "hands-on-litellm",
  "version": "1.0.0",
  "description": "Working with LLM through LiteLLM",
  "author": "Damir Manapov",
  "license": "MIT",
  "type": "module",
  "dependencies": {
    "openai": "^6.9.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.6.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.47.0",
    "vitest": "^4.0.13"
  },
  "scripts": {
    "build": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "compose:up": "docker compose up -d starter",
    "compose:down": "docker compose down",
    "compose:restart": "docker compose restart",
    "compose:reset": "docker compose down -v --remove-orphans"
  }
}
